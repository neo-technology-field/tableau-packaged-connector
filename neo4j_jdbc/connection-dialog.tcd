<connection-dialog class='neo4j_jdbc'>
  <connection-config>
     <authentication-mode value='Basic' />
    <authentication-options>
      <option name="UsernameAndPassword" default="true" value="auth-user-pass" />
    </authentication-options>
    <has-schemas value="true" />
    <port-prompt value="Port: " default="7687" />
    <service-prompt value="Driver Options: " />
  </connection-config>
</connection-dialog>
